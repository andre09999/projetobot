{
  "name": "api-telegram",
  "version": "1.0.0",
  "description": "",
  "main": "./index.js",
  "scripts": {
    "start": "npm run db:init && node ./index.js",
    "vercel-build": "npm run db:init deploy",
    "dev": "npm run db:init && nodemon ./index.js",
    "db:init": "npx sequelize db:create && npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "reset": "npx sequelize-cli db:migrate:undo:all && npm run db:init",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.23.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.63.0",
    "sequelize": "^6.6.2",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.1",
    "telegraf": "^1",
    "telegram-node-bot": "^4.0.5"
  },
  "devDependencies": {
    "nodemon": "2.0.15",
    "sequelize-cli": "^6.6.1"
  }
}
